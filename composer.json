{
    "name": "nirvana72/phpboot-ext",
    "description": "基于caoym/phpboot原作者的一些个人需求改造",
    "keywords": [
        "phpboot",
        "restful",
        "boot"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "monolog/monolog": "~1.0",
        "symfony/http-foundation": "4.4",
        "symfony/http-kernel": "4.4",
        "symfony/console": "~4.0",
        "vlucas/valitron": "^1.4",
        "doctrine/cache": "^1.6",
        "phpdocumentor/reflection-docblock": "^4.3",
        "mtdowling/jmespath.php": "^2.4",
        "nikic/fast-route": "^1.2",
        "php-di/php-di": "^5.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "authors": [
        {
            "name": "Yangmin Cao",
            "email": "caoyangmin@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpBoot\\": "src/",
            "PhpBoot\\Tests\\": "tests/"
        },
        "files": [
            "src/functions.php",
            "src/DB/rules/basic.php",
            "src/DB/rules/delete.php",
            "src/DB/rules/insert.php",
            "src/DB/rules/replace.php",
            "src/DB/rules/select.php",
            "src/DB/rules/update.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}